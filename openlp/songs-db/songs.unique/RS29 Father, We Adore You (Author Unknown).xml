  <properties>
    <titles>
      <title>RS29 Father, We Adore You</title>
    </titles>
    <verseOrder>v1 v2 v3</verseOrder>
    <authors>
      <author>Author Unknown</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines><br/>Father, we adore You,<br/>lay our lives before You;<br/>how we love You!</lines>
    </verse>
    <verse name="v2">
      <lines><br/>Jesus, we adore You<br/>lay our lives before You;<br/>how we love You!</lines>
    </verse>
    <verse name="v3">
      <lines><br/>Spirit, we adore You<br/>lay our lives before You;<br/>how we love You!</lines>
    </verse>
  </lyrics>
</song>

<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2018-12-09T19:21:06">
  <properties>
    <titles>
      <title>SF374 Come, Holy Spirit,</title>
    </titles>
    <verseOrder>v1 v2 v3 v4 v5 v6 v7 v8</verseOrder>
    <authors>
      <author>John L. Bell (b. 1949)</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines><br/>Come, Holy Spirit,<br/>descend on us,<br/>descend on us.<br/>We gather here in Jesus' name.</lines>
    </verse>
    <verse name="v2">
      <lines><br/>Come, Breath of Heaven,</lines>
    </verse>
    <verse name="v3">
      <lines><br/>Come, Word of Mercy,</lines>
    </verse>
    <verse name="v4">
      <lines><br/>Come, Fire of Judgement,</lines>
    </verse>
    <verse name="v5">
      <lines><br/>Come, Great Creator,</lines>
    </verse>
    <verse name="v6">
      <lines><br/>Come to unite us,</lines>
    </verse>
    <verse name="v7">
      <lines><br/>Come to disturb us,</lines>
    </verse>
    <verse name="v8">
      <lines><br/>Come to inspire us,</lines>
    </verse>
  </lyrics>
</song>
